   "source": [
    "def projToSky(X,Y,alpha0,delta0):\n",
    "    alpha = np.arctan(-(X)/(np.cos(delta0)-Y*np.sin(delta0)))+alpha0\n",
    "    delta = np.arcsin((np.sin(delta0)+Y*np.cos(delta0))/(1+X**2+Y**2)**.5)\n",
    "    \n",
    "    return np.array([alpha,delta])"
   ]
  }
